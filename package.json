{
  "name": "eff",
  "private": true,
  "version": "1.0.0",
  "workspaces": [
    "backend/services/*",
    "services/*"
  ],
  "resolutions": {
    "@types/express": "4.17.21",
    "express": "4.19.2",
    "zod": "4.0.17"
  },
  "devDependencies": {
    "@types/axios": "^0.14.4",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.3.0",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "concurrently": "^8.0.0",
    "dotenv": "^17.2.1",
    "dotenv-cli": "^10.0.0",
    "supertest": "^7",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "scripts": {
    "dev": "./run.sh dev",
    "docker": "./run.sh docker",
    "test:act:unit": "vitest run -c backend/services/act/vitest.config.ts",
