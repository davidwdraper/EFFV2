{
  "extends": "../../../tsconfig.json",
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "moduleDetection": "force",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "lib": ["ES2022", "DOM"],
    "types": ["node", "vitest"],
    "rootDir": "..",
    "outDir": "../dist/log"
  },
  "include": ["src", "index.ts", "../shared/**/*.ts", "test", "types"]
}
