{
  "name": "svcconfig-service",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register --project tsconfig.json index.ts",
    "dev": "TS_NODE_FILES=1 ts-node-dev -r tsconfig-paths/register --respawn --transpile-only index.ts",
    "build": "tsc -p tsconfig.json",
    "seed": "TS_NODE_FILES=1 ts-node -P tsconfig.json -r tsconfig-paths/register src/seeds/seed.ts",
    "test": "vitest run"
  },
  "dependencies": {
    "@eff/shared": "workspace:*",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.6.0",
    "pino-http": "^10.5.0",
    "redis": "^4.6.13",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.30",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  }
}
