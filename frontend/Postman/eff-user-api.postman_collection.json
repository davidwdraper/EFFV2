{
  "info": {
    "name": "EFF User API",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Login",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"email\": \"user@example.com\",\n  \"password\": \"password123\"\n}"
        },
        "url": {
          "raw": "http://localhost:4000/auth/login",
          "protocol": "http",
          "host": [
            "localhost"
          ],
          "port": "4000",
          "path": [
            "api",
            "auth",
            "login"
          ]
        }
      },
      "response": []
    },
    {
      "name": "Search Users",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "http://localhost:4000/users/search?q=test",
          "protocol": "http",
          "host": [
            "localhost"
          ],
          "port": "4000",
          "path": [
            "api",
            "users",
            "search"
          ],
          "query": [
            {
              "key": "q",
              "value": "test"
            }
          ]
        }
      },
      "response": []
    },
    {
      "name": "Get User by ID",
      "request": {
        "method": "GET",
        "header": [
          {
            "key": "Authorization",
            "value": "Bearer {{jwt_token}}"
          }
        ],
        "url": {
          "raw": "http://localhost:4000/users/{{userId}}",
          "protocol": "http",
          "host": [
            "localhost"
          ],
          "port": "4000",
          "path": [
            "api",
            "users",
            "{{userId}}"
          ]
        }
      },
      "response": []
    }
  ],
  "variable": [
    {
      "key": "jwt_token",
      "value": ""
    },
    {
      "key": "userId",
      "value": "user-guid-here"
    }
  ]
}