{
  "name": "@eff/shared",
  "private": true,
  "type": "commonjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist tsconfig.tsbuildinfo || true",
    "build": "pnpm run clean && tsc -b",
    "typecheck": "tsc -b --noEmit"
  },
  "//": "No barrel export for '.'. Only explicit subpaths.",
  "exports": {
    "./bootstrap/*": "./dist/bootstrap/*.js",
    "./env": "./dist/env.js",
    "./svcconfig/*": "./dist/svcconfig/*.js",
    "./utils/*": "./dist/utils/*.js",
    "./contracts/*": "./dist/contracts/*.js",
    "./package.json": "./package.json",
    "./src/*": "./dist/*.js"
  },
  "typesVersions": {
    "*": {
      "bootstrap/*": [
        "dist/bootstrap/*.d.ts"
      ],
      "env": [
        "dist/env.d.ts"
      ],
      "svcconfig/*": [
        "dist/svcconfig/*.d.ts"
      ],
      "utils/*": [
        "dist/utils/*.d.ts"
      ],
      "contracts/*": [
        "dist/contracts/*.d.ts"
      ],
      "*": [
        "dist/index.d.ts"
      ]
    }
  },
  "devDependencies": {
    "rimraf": "^5.0.5",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "undici": "^7.16.0"
  }
}
